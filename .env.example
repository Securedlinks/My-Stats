# Environment Variables for COLDBIND Discord Status Page
# Copy this file to .env and fill in your values

# Discord Bot Token (from Discord Developer Portal > Bot section)
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Discord Server/Guild ID
DISCORD_SERVER_ID=1378505276470202428

# Discord User ID (the owner's user ID)
DISCORD_USER_ID=978118519097028628

# Discord Application Client ID (from Discord Developer Portal > OAuth2)
DISCORD_CLIENT_ID=your_discord_client_id_here

# Server Port (Render will set this automatically)
PORT=10000

# Node Environment
NODE_ENV=production
